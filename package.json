{
  "name": "ng2-datepicker",
  "version": "1.1.1",
  "description": "Angular2 Datepicker",
  "main": "ng2-datepicker.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jkuri/ng2-datepicker.git"
  },
  "scripts": {
    "prepublish": "tsc",
    "start": "tsc && concurrently \"npm run lite\" \"npm run tsc:w\" ",
    "lite": "lite-server",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "copy:bs": "copyfiles -f node_modules/bootstrap/dist/css/bootstrap.min.css node_modules/bootstrap/dist/css/bootstrap-theme.min.css src/assets/css && copyfiles -f node_modules/bootstrap/dist/fonts/** src/assets/fonts",
    "postinstall": "npm run copy:bs"
  },
  "keywords": [
    "angular",
    "angular2",
    "datepicker",
    "date",
    "directive"
  ],
  "author": "Jan Kuri <jkuri88@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@angular/common": "2.0.0",
    "@angular/compiler": "2.0.0",
    "@angular/core": "2.0.0",
    "@angular/forms": "^2.0.0",
    "@angular/platform-browser": "2.0.0",
    "concurrently": "^2.2.0",
    "copyfiles": "^1.0.0",
    "lite-server": "^2.2.2",
    "rxjs": "5.0.0-beta.12",
    "typescript": "^2.0.2",
    "zone.js": "0.6.21"
  },
  "dependencies": {
    "@types/moment": "^2.11.29",
    "bootstrap": "^3.3.7",
    "moment": "^2.14.1",
    "reflect-metadata": "^0.1.8",
    "systemjs": "^0.19.38"
  }
}
